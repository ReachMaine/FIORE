{% comment %} CSS that needs snippets {% endcomment %}
<style type="text/css">
{% unless settings.panel_bg_opacity == '0' or settings.panel_bg_opacity == '1' or settings.panel_bg_opacity == empty %}
    /* Translucent background */
    {% assign hex2rgb_hex = settings.panel_bg_col %}
    {% include 'hex2rgb' %}

    #navbar,
    #mobilefooter {
        background:rgba({{ hex2rgb_r }},{{ hex2rgb_g }},{{ hex2rgb_b }},{{ settings.panel_bg_opacity }});
    }
    #navbar .search input[type=text],
    .footer .registerform input[type=email] {
        background:transparent;
    }
{% endunless %}
</style>
{% comment %} lms add flexslider to get slideshow on homepage {% endcomment %}

<script>
   $(document).ready (function() {
	{% comment %}	$('.flexslider.homepageflexslider').flexslider({
              animation: "slide",
              start: function(){ //On first slide
                   $('.masonry').masonry();
              },
              after: function(){ //After subsequent slides - not needed if all slides are the same size
                  $('.masonry').masonry();
              }
        });
{% endcomment %}
       $('.flexslider.ColWBFlexSlider').flexslider({
			animation: "slide",
            controlsContainer: ".flex-container",
  			start: function(){ //On first slide
           		 $('.masonry').masonry();
            },
			after: function(){ //After subsequent slides - not needed if all slides are the same size
                 $('.masonry').masonry();
			}
        });
      $('.flexslider.ColSFFlexSlider').flexslider({
			animation: "slide",
            controlsContainer: ".flex-container",
			start: function(){ //On first slide
           		 $('.masonry').masonry();
            },
 			after: function(){ //After subsequent slides - not needed if all slides are the same size
                    $('.masonry').masonry();
			}
     	});
  	});

	$(window).load (function () { // lms - makesure navbar is tall enough after window load
	   $('#navbar').css('min-height',Math.max($(window).height(), $('#navpanel').outerHeight()));
	   $('.flexslider').flexslider();
  });

</script>

</head>

<body class="{% if customer %}customer-logged-in {% endif %}template-{{ template | split: '.' | first }}">
  <!-- Google Tag Manager -->
<noscript><iframe src="//www.googletagmanager.com/ns.html?id=GTM-TLWNXT"
