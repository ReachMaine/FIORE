}

.block .readmore span {
    position:relative;
    top:0.7em;
    background-color:{{ settings.block_bg }};
    padding:1em 1.5em;
}

.block .fullarticle {
    margin:1em 0 1em;
}

.block .pagetitle {
    margin-top:0;
}

.block .pagetitle.nocontent {
    margin-bottom:0;
}

.block.product p {
    margin-bottom:0.5em;
    line-height:1.4em;
}

.block.product .sub {
    border-color:{{ settings.block_prod_div_sub }};
    background:{{ settings.block_prod_sub_bg }};
    padding:{{ product_block_padding }}px;
    position:relative;
}

.block.product .sub .title {
    color:{{ settings.prod_block_title_col }};
  padding-right: 17px {% comment %} lms s.t. expand button doesnt cover title {% endcomment %}
}

.block.product:hover .sub .title {
    color:{{ settings.prod_block_title_hov_col }};
}

.block.product .sub.quickbuyarea {
    display:none;
}

.block.product .sub.quickbuyarea .row {
    margin-bottom:15px;
}

.block.product .sub.quickbuyarea .quantityrow label {
    display:inline;
    margin-right:8px;
}

.block.product .sub.quickbuyarea .quantityrow input {
    width:20px;
}

.block.product .sub.quickbuyarea input[type=text],
.block.product .sub.quickbuyarea input[type=number] {
    height:22px;
    line-height:22px;
    text-align:center;
}

.product .was-price {
    font-style:italic;
    margin-right:14px;
    padding-right:1px;/* avoid Chrome font clip */
    text-decoration:line-through;
    opacity:0.8;
    filter: alpha(opacity = 80);
    zoom:1;
}

.product .from {
    font-style:italic;
    margin-right:5px;
    opacity:0.8;
    filter: alpha(opacity = 80);
    zoom:1;
}

.block.padded,
.block .padded {
    padding:{{ general_block_padding }}px;
}

.block .sidesonly {
    padding-top:0;
    padding-bottom:0;
}

.block.double-padded,
.block .double-padded {
    padding:{{ general_block_padding | times: 2 }}px;
}
  {% comment %}  
.block .sub.double-padded,
  {% endcomment %} 
.block .double-padded.sidesonly {
    padding-top:{{ general_block_padding }}px;
    padding-bottom:{{ general_block_padding }}px;
}

.block .main.contactpagecontent {
    margin-bottom:0;
    padding-bottom:0;
}

#navbar {
    position:absolute;
    left:0;
